{
    "name": "awcodes/trov-discoveries",
    "type": "project",
    "description": "Discovery Center Resources for integration into Trov CMS",
    "keywords": [
        "framework",
        "laravel",
        "filament",
        "trov",
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "filament/filament": "^2.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "awcodes/trov": "dev-main",
        "awcodes/trov-components": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Trov\\Discoveries\\": "src/",
            "Trov\\Discoveries\\Database\\Factories\\": "database/factories/",
            "Trov\\Discoveries\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trov\\Discoveries\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trov\\Discoveries\\TrovDiscoveriesServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}